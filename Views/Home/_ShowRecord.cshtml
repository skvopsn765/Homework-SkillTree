@model dynamic

<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tr>
                <th>#</th>
                <th>類別</th>
                <th>日期</th>
                <th>金額</th>
            </tr>
            @{
                var i = 1;
                foreach (var item in @Model)
                {
                    <tr>
                        <td>@(i++)</td>
                        <td>@item.Category</td>
                        <td>@item.Date</td>
                        <td>@item.Money</td>
                    </tr>
                }
            }
            <tr>
                <td>2</td>
                <td>支出</td>
                <td>2016-01-02</td>
                <td>1,600</td>
            </tr>
            <tr>
                <td>3</td>
                <td>支出</td>
                <td>2016-01-03</td>
                <td>8,00</td>
            </tr>
        </table>
    </div>
</div>